package print_following_pattern;

public class TrianglePattern 
{
	public static void main(String[] args) 
	{
		int n=5;
		for(int i=0;i<n;i++)
		{
			for(int j=n-i;j>1;j--)
			{
				System.out.println();
			}
			for(int j=0;j<=1;j++)
			{
				System.out.println("*");
			}
			
			System.out.println();
		}
			
	}

}
