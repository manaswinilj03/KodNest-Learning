import java.util.Scanner;

public class Main3 
{
	public static void main(String[] args) 
	{
		Scanner scan=new Scanner(System.in);
		int num=scan.nextInt();
		printDigit(num);
		
	}
	public static void printDigit(int num) 
	{
		int count=0;
		while(count==num)
		{
			System.out.println(count);
			count++;
		}
		
		
	}

}
